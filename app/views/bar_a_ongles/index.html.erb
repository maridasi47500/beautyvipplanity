<p id="notice"><%= notice %></p>

<h1>Bar A Ongles</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Prestation info</th>
      <th>Prestationtype</th>
      <th>Prestationtypecolumn</th>
      <th>Price</th>
      <th>Length</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bar_a_ongles.each do |bar_a_ongle| %>
      <tr>
        <td><%= bar_a_ongle.name %></td>
        <td><%= bar_a_ongle.prestation_info_id %></td>
        <td><%= bar_a_ongle.prestationtype_id %></td>
        <td><%= bar_a_ongle.prestationtypecolumn %></td>
        <td><%= bar_a_ongle.price %></td>
        <td><%= bar_a_ongle.length %></td>
        <td><%= link_to 'Show', bar_a_ongle %></td>
        <td><%= link_to 'Edit', edit_bar_a_ongle_path(bar_a_ongle) %></td>
        <td><%= link_to 'Destroy', bar_a_ongle, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bar A Ongle', new_bar_a_ongle_path %>
